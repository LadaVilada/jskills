<html xmlns:wicket="http://wicket.sourceforge.net/">
<wicket:link>
	<link rel="stylesheet" href="css/add.css" type="text/css" />
</wicket:link>
<body>
	<wicket:extend>
		<span wicket:id="feedback"></span>
		<form wicket:id="loginForm">

			<input wicket:id="username" value="User name" type="text" /> 
			<input wicket:id="password" type="password"/> 
			<input
				wicket:id="submitButton" type="submit" value="Login"
				class="btn-custom" />

		</form>
		<div wicket:id="regWindow"></div>
		<a wicket:id="showRegistration">Registration</a>
		<a wicket:id="forgot">Forgot password</a>
	</wicket:extend>
</body>
</html>