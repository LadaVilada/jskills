<html xmlns:wicket="http://wicket.sourceforge.net/">
<body>
	<wicket:extend>	
		
		<form wicket:id="usersListForm">
			<h1><span wicket:id="message" class="header-custom">Message goes here</span></h1>
			<br>
			<table title="Users List" wicket:id="datatable" class="table table-striped table-bordered table-hover table-condensed" border="1"></table>		
			<br>
			<input wicket:id="createUserButton" type="submit" value="Create User" class="btn-primary"/>
			<br>
		</form>
	</wicket:extend>
</body>
</html>
